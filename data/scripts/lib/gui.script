   var elmSliderBtnHandle : Integer = GetGUIElementIndexByNameParent('btnSlider', elmSliderHandle);
   SetGUIElementPositionX(elmSliderBtnHandle, sliderXOffset+round(value*sliderWidth));
   Result := value;
